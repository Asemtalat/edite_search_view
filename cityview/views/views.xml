<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_template_city_view_tree" model="ir.ui.view">
            <field name="name">product2 template2</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_city"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_valve"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_manifacture_date"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_capacityl"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_"/>
                </xpath>
            </field>
        </record>

        <record id="product_template_city_view_tree_search" model="ir.ui.view">
            <field name="name">product product</field>
            <field name="model">product.product</field>
            <field eval="1" name="priority"/>
            <field name="inherit_id" ref="product.product_product_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_city"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_valve"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_manifacture_date"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_capacityl"/>
                </xpath>
                <xpath expr="//tree" position="inside">
                    <field name="x_studio_"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>



